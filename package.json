{
  "name": "steam-randomizer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pretest": "NODE_ENV=test API_KEY= DB_TEST_HOST=mongodb://localhost/steam-randomizer-test ./node_modules/mocha/bin/mocha tests/*",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 API_KEY= DB_TEST_HOST=mongodb://localhost/steam-randomizer-test ./node_modules/mocha/bin/mocha --require blanket --reporter mocha-lcov-reporter tests/* | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "mongodb": "^2.0.49",
    "passport-steam": "^0.1.6",
    "express-session": "^1.12.1",
    "passport": "^0.3.2",
    "mongoose": "^4.2.9",
    "steam-web": "^0.2.5",
    "dotenv": "^1.2.0",
    "connect-mongo": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "async": "^1.5.0",
    "sinon": "^1.17.2",
    "mockery": "^1.4.0",
    "rewire": "^2.5.1",
    "mocha": "^2.3.4",
    "coveralls": "^2.11.6",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.1",
    "gulp-watch": "^4.3.5",
    "gulp-minify-css": "^1.2.3",
    "gulp-rename": "^1.2.2",
    "mocha-lcov-reporter": "^1.0.0",
    "blanket": "^1.2.1",
    "gulp-minify": "0.0.5"
  }
}
